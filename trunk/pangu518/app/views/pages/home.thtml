<script> 
  var speed=2//速度数值越大速度越慢 
  demo2.innerHTML=demo1.innerHTML 
  function Marquee(){ 
  a=1
  if(demo2.offsetWidth-demo.scrollLeft<=0) {
  demo.scrollLeft-=demo1.offsetWidth
  }else{ 
  demo.scrollLeft++ 
  } 
  } 
  function Marquee1(){ 
  a=2
  if(demo.scrollLeft<=0) {
  demo.scrollLeft+=demo1.offsetWidth
  }else{ 
  demo.scrollLeft-- 
  } 
  } 
  var MyMar=setInterval(Marquee,speed) 
  demo.onmouseover=function() {clearInterval(MyMar)} 
  demo.onmouseout=function() {
  if(a==1){
  MyMar=setInterval(Marquee,speed)
  }else{
  MyMar=setInterval(Marquee1,speed)
  }
  }
  upbutton.onmouseover=function(){
     clearInterval(MyMar)
     MyMar=setInterval(Marquee,speed)
	 this.src="images/pic_left_buttonover.gif"
  }
   upbutton.onmouseout=function(){
	 this.src="images/pic_left_button.gif"
  }
  downbutton.onmouseover=function(){
     clearInterval(MyMar)
     MyMar=setInterval(Marquee1,speed)
	 this.src="images/pic_right_buttonover.gif"
  }
  downbutton.onmouseout=function(){
     clearInterval(MyMar)
     MyMar=setInterval(Marquee1,speed)
	 this.src="images/pic_right_button.gif"
  }
</script>
